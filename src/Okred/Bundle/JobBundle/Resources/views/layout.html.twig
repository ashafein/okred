<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8"/>
		<title>{% block title %}{% endblock %}</title>
		<!-- Bootstrap -->
		{% block stylesheets  %}
		"{{ asset('bundles/acmedemo/css/demo.css') }}"
		<link href="{{ asset('bundles/okred/css/reset.css') }}" rel="stylesheet" media="screen">
		<link href="{{ asset('bundles/okred/css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
		<link href="{{ asset('bundles/okred/css/fonts.css') }}" rel="stylesheet" media="screen">
		{% endblock %}
	</head>
	<body>
		<script src="http://code.jquery.com/jquery-latest.js"></script>
		<script src="js/bootstrap.min.js"></script>
		<!-- JS -->
		<div class="container" id="main-wrapper">
			<div id="main-column">
			
			{% block registration %}
			{% endblock %}
			
			{% block content %}
			{% endblock %}
			</div>
			<div id="extra-column">
			</div>
		</div>
	</body>
</html>