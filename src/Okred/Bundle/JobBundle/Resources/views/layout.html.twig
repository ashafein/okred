    <title>{% block title %}{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('/bundles/okredjob/css/reset.css') }}" rel="stylesheet" media="screen">
        <link href="{{ asset('/bundles/okredjob/css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
        <link href="{{ asset('/bundles/okredjob/css/fonts.css') }}" rel="stylesheet" media="screen">
    {% endblock %}
</head>
<body>
<script src="http://code.jquery.com/jquery-latest.js"></script>
<script src="{{ asset('/bundles/okredjob/js/bootstrap.min.js') }}"></script>
<!-- JS -->
<div class="container" id="main-wrapper">
    <div id="main-column">

        {% block registration %}
        {% endblock %}

        {% block content %}
        {% endblock %}
    </div>
    <div id="extra-column">
    </div>
</div>
</body>
</html>